{
  "name": "onoffv3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "db:create": "psql -U postgres -c \"CREATE DATABASE timetec_hrv2;\"",
    "db:drop": "psql -U postgres -c \"DROP DATABASE IF EXISTS timetec_hrv2;\"",
    "db:schema": "psql -U postgres -d timetec_hrv2 -f database/schema.sql",
    "db:seed": "psql -U postgres -d timetec_hrv2 -f database/seed.sql",
    "db:setup": "npm run db:create && npm run db:schema && npm run db:seed",
    "db:reset": "npm run db:drop && npm run db:setup"
  },
  "dependencies": {
    "@primevue/themes": "^4.4.0",
    "chart.js": "^4.5.0",
    "lucide-vue-next": "^0.545.0",
    "pinia": "^3.0.3",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.0",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "vite": "^7.1.7"
  }
}
